# DEVELOPMENT ENVIRONMENT (.env.development.local)
# Deployment used by `npx convex dev`
CONVEX_DEPLOYMENT=dev:your-dev-deployment # team: yourteam, project: yourproject
NEXT_PUBLIC_CONVEX_URL=https://your-dev-deployment.convex.cloud

# Development GitHub OAuth (localhost:3000)
AUTH_GITHUB_ID="your_dev_github_client_id"
AUTH_GITHUB_SECRET="your_dev_github_client_secret"

# Development Google OAuth (localhost:3000)
AUTH_GOOGLE_ID="your_dev_google_client_id"
AUTH_GOOGLE_SECRET="your_dev_google_client_secret"

# Stripe Test Keys (for development)
STRIPE_SECRET_KEY=sk_test_your_test_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_dev_webhook_secret

# ================================================

# PRODUCTION ENVIRONMENT (.env.production.local)
# Production Convex deployment
CONVEX_DEPLOYMENT=prod:your-prod-deployment # team: yourteam, project: yourproject
NEXT_PUBLIC_CONVEX_URL=https://your-prod-deployment.convex.cloud

# Production GitHub OAuth (your-domain.vercel.app)
AUTH_GITHUB_ID="your_prod_github_client_id"
AUTH_GITHUB_SECRET="your_prod_github_client_secret"

# Production Google OAuth (your-domain.vercel.app)
AUTH_GOOGLE_ID="your_prod_google_client_id"
AUTH_GOOGLE_SECRET="your_prod_google_client_secret"

# Stripe Live Keys (for production)
STRIPE_SECRET_KEY=sk_live_your_live_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_prod_webhook_secret

# Required for production deployment
CONVEX_DEPLOY_KEY="prod:your-prod-deployment|your_deploy_key_here"